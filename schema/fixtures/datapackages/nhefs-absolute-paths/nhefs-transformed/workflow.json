{
	"$schema": "https://microsoft.github.io/datashaper/schema/workflow/v2.json",
	"profile": "workflow",
	"name": "nhefs-transformed/workflow.json",
	"title": "NHEFS Workflow",
	"steps": [
		{
			"args": {
				"strategy": "auto",
				"fixedcount": 10,
				"to": "ht-bin",
				"column": "ht",
				"printRange": true
			},
			"verb": "bin"
		},
		{
			"args": {
				"to": "high-cholesterol",
				"criteria": {
					"operator": ">",
					"type": "value",
					"value": "200"
				},
				"column": "cholesterol"
			},
			"verb": "binarize"
		}
	]
}
