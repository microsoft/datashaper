{
	"$schema": "https://microsoft.github.io/datashaper/schema/workflow/v1.json",
	"name": "nhefs-transformed/workflow.json",
	"title": "NHEFS Workflow",
	"input": [
		"TODO: We're referencing the datatable json definition, which should be materialized before passing into the workflow",
		"nhefs/datatable.json"
	],
	"output": [
		{
			"node": "bin",
			"name": "bin"
		},
		{
			"node": "binarize",
			"name": "binarize"
		}
	],
	"steps": [
		{
			"id": "bin",
			"args": {
				"strategy": "auto",
				"fixedcount": 10,
				"to": "ht-bin",
				"column": "ht",
				"printRange": true
			},
			"verb": "bin",
			"input": {
				"source": {
					"node": "nhefs.csv"
				}
			}
		},
		{
			"id": "binarize",
			"args": {
				"to": "high-cholesterol",
				"criteria": [
					{
						"operator": ">",
						"type": "value",
						"value": "200"
					}
				],
				"logical": "or",
				"column": "cholesterol"
			},
			"verb": "binarize",
			"input": {
				"source": {
					"node": "bin"
				}
			}
		}
	]
}
