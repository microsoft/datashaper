{
	"name": "@datashaper/utilities",
	"version": "2.4.0",
	"type": "module",
	"main": "src/index.ts",
	"publishConfig": {
		"main": "dist/index.js",
		"types": "dist/index.d.ts"
	},
	"sideEffects": false,
	"author": "Nathan Evans <naevans@microsoft.com>",
	"license": "MIT",
	"contributors": [
		"Nathan Evans <naevans@microsoft.com>",
