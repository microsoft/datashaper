		"@types/debug": "^4.1.7",
		"@types/jest": "^29.2.6",
		"@types/node": "^18.11.18",
		"arquero": "^5.1.0",
		"cross-blob": "^3.0.2",
		"cross-fetch": "^3.1.5",
		"jest": "^29.3.1",
		"jest-environment-jsdom": "^29.3.1",
		"typescript": "^4.9.4"
